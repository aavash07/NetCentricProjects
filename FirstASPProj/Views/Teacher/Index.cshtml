@model List<TeacherModel>

<table class="table table-striped">
    <thead>
        <tr>
            <td>Name</td>
            <td>Subject</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.name</td>
                <td>@teacher.subject</td>
            </tr>
        }
    </tbody>
</table>
